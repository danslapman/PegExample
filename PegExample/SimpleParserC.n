using Nemerle;
using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;
using Nemerle.Peg;

using System;
using System.Collections.Generic;
using System.Linq;

namespace PegExample
{
   [PegGrammar(Options = EmitDebugSources, 
    number, 
    grammar {
      d : int = ['0' .. '9'];
      number : int = d d;
  })]
  class SimpleParserC
  {
    d(token : NToken) : int {
      int.Parse(GetText(token))
    }
    
    number(a : int, b : int) : int {
      a * 10 + b
    }
  }
}

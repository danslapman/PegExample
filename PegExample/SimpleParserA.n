using Nemerle;
using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;
using Nemerle.Peg;

using System;
using System.Collections.Generic;
using System.Linq;

namespace PegExample
{
  [PegGrammar(Options = EmitDebugSources, 
    d, 
    grammar {
      d : int = "0";
  })]
  class SimpleParserA
  {
    d(token : NToken) : int {
      int.Parse(GetText(token))
    }
  }
}

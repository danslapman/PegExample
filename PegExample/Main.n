using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Console;
using System.Linq;

using PegExample;

module Program
{
  Main() : void
  {
    def parser = SimpleParserH();
    while(true) {
      match(parser.Parse(ReadLine())) {
        | Some(ast) => WriteLine($"good: $ast")
        | None() => WriteLine("error")
      }
    }
  }
}